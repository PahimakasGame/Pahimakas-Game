parameters":["    Pilipinas."]},{"code":101,"indent":0,"parameters":["",0,2,1,""]},{"code":401,"indent":0,"parameters":["    Ngayon, alamin natin kung paano laruin ang Pahimakas."]},{"code":101,"indent":0,"parameters":["",0,2,1,""]},{"code":401,"indent":0,"parameters":["    Pumindot sa kahit saang bahagi ng iyong “screen”."]},{"code":101,"indent":0,"parameters":["",0,2,1,""]},{"code":401,"indent":0,"parameters":["    Maaari mo ang mga letrang ‘W’, ‘A’, ‘S’, at ‘D’ sa"]},{"code":401,"indent":0,"parameters":["    iyong tipaan."]},{"code":101,"indent":0,"parameters":["",0,2,1,""]},{"code":401,"indent":0,"parameters":["    Hanapin mo ang istatuwa sa loob ng kwarto na ito."]},{"code":214,"indent":0,"parameters":[]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true}],"x":1,"y":4},
